CREATE OR REPLACE PACKAGE [@ELEMENT_NAME@]
IS
	/*
	|| Package : [@ELEMENT_NAME@]
	||
	|| Auteur : [@ELEMENT_AUTHOR@] - [@ELEMENT_EMAIL@]
	|| Date : [@ELEMENT_DATE@]
	||
	|| But : [@ELEMENT_DESC@]
	||
	||---< Maintenance >---------------------------------------------------------
	*/

	--#-< Déclaration des types >-######-------------------
	--#-< Déclaration des variables >-######---------------
	--#-< Déclaration des curseurs >-######----------------
	--#-< Déclaration des fonctions et procédures >-######-

END [@ELEMENT_NAME@];
/

DROP PUBLIC SYNONYM [@ELEMENT_NAME@]
/

CREATE PUBLIC SYNONYM [@ELEMENT_NAME@] FOR [@ELEMENT_NAME@]
/

GRANT ALL ON [@ELEMENT_NAME@] TO Developper
/

GRANT EXECUTE ON [@ELEMENT_NAME@] TO Appli_User
/

