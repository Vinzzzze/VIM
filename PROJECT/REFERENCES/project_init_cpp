CURRENT_PROJECT=[@ELEMENT_MACRO@]
EXPORT_HOME=${BASE_EXPORT}/${CURRENT_PROJECT}
PROJECT_HOME=${BASE_PROJECT}/${LOCAL_PROJECT}/${CURRENT_PROJECT}
BUILD_HOME=${BASE_BUILD}/${CURRENT_PROJECT}

export CURRENT_PROJECT PROJECT_HOME BUILD_HOME EXPORT_HOME

cd ${BUILD_HOME}

echo "Welcome to the project [${CURRENT_PROJECT}]"

alias cdm='cd ${BUILD_HOME}'
alias cdg="cd ${BUILD_HOME}/MAKEFILEGENERATION"
alias gm="php -f ${BASE_PROJECT}/${LOCAL_PROJECT}/MAKEFILEGENERATION/manageDependencies_gccMingW.php"
alias outLib=${PROJECT_HOME}/export.sh

