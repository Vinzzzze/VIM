                 [@ELEMENT_BLANK@]           char*                      buffer,
                 [@ELEMENT_BLANK@]           int_StreamObjectAllocator* allocator,
                 [@ELEMENT_BLANK@]           char*                      reference,
                 [@ELEMENT_BLANK@]           data_ObjectDescription*    referenceDesc );

